ude\" :label=\"$t('longitude')\" outlined dense hide-details class=\"custom-input\"\n                type=\"number\" suffix=\"°\" @input=\"handleInput\" @touchstart.stop @touchmove.stop @touchend.stop></v-text-field>\n            <v-text-field v-model=\"latitude\" :label=\"$t('latitude')\" outlined dense hide-details class=\"custom-input\"\n                type=\"number\" suffix=\"°\" @input=\"handleInput\" @touchstart.stop @touchmove.stop @touchend.stop></v-text-field>\n            <v-text-field v-model=\"focalLength\" :label=\"$t('focalLength')\" outlined dense hide-details\n                class=\"custom-input\" type=\"number\" suffix=\"mm\" @input=\"handleInput\" @touchstart.stop @touchmove.stop @touchend.stop></v-text-field>\n        </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    name: 'LocationFocalInputs',\n    props: {\n        showInputs: {\n            type: Boolean,\n            default: false\n        }\n    },\n    data() {\n        return {\n            longitude: '',\n            latitude: '',\n            focalLength: ''\n        }\n    },\n    methods: {\n        handleInput() {\n            // 确保值是数字\n            const longitude = parseFloat(this.longitude) || 0;\n            const latitude = parseFloat(this.latitude) || 0;\n            const focalLength = parseFloat(this.focalLength) || 0;\n\n            // 验证范围\n            if (longitude < -180 || longitude > 180) {\n                this.longitude = '';\n                return;\n            }\n            if (latitude < -90 || latitude > 90) {\n                this.latitude = '';\n                return;\n            }\n            if (focalLength < 0) {\n                this.focalLength = '';\n                return;\n            }\n\n            // 发送更新事件\n            this.$emit('location-update', {\n                longitude,\n                latitude,\n                focalLength\n            });\n        }\n    }\n}\n</script>\n\n<style scoped>\n.top-left-inputs {\n    position: absolute;\n    top: 10px;\n    left: 10px;\n    z-index: 1000;\n    pointer-events: auto; /* 确保可以接收鼠标事件 */\n    touch-action: none; /* 禁用默认触摸行为 */\n}\n\n.input-container {\n    display: flex;\n    flex-direction: column;\n    gap: 8px;\n    background-color: rgba(0, 0, 0, 0.5);\n    padding: 10px;\n    border-radius: 4px;\n    pointer-events: auto; /* 确保可以接收鼠标事件 */\n    touch-action: none;\n}\n\n.custom-input {\n    width: 200px;\n    pointer-events: auto; /* 确保可以接收鼠标事件 */\n    touch-action: none;\n}\n\n:deep(.v-text-field) {\n    background-color: rgba(255, 255, 255, 0.1);\n    pointer-events: auto; /* 确保可以接收鼠标事件 */\n    touch-action: none;\n}\n\n:deep(.v-text-field .v-input__control) {\n    color: white;\n    pointer-events: auto; /* 确保可以接收鼠标事件 */\n    touch-action: none;\n}\n\n:deep(.v-text-field .v-label) {\n    color: rgba(255, 255, 255, 0.7);\n}\n\n:deep(.v-text-field .v-input__slot) {\n    border: 1px solid rgba(255, 255, 255, 0.3);\n    pointer-events: auto; /* 确保可以接收鼠标事件 */\n    touch-action: none;\n}\n\n/* 确保输入框可以接收焦点和触摸 */\n:deep(.v-text-field input) {\n    pointer-events: auto;\n    color: white;\n    touch-action: none;\n    -webkit-tap-highlight-color: transparent; /* 移除触摸高亮 */\n}\n\n/* 添加触摸反馈样式 */\n:deep(.v-text-field:active) {\n    background-color: rgba(255, 255, 255, 0.2);\n}\n\n/* 确保数字输入框的上下箭头可见 */\n:deep(.v-text-field input[type=\"number\"]::-webkit-inner-spin-button),\n:deep(.v-text-field input[type=\"number\"]::-webkit-outer-spin-button) {\n    opacity: 1;\n    pointer-events: auto;\n    touch-action: none;\n}\n</style>\n"]}]}